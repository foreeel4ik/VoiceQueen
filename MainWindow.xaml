<Window x:Class="VoiceQueen.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="VoiceQueen" Height="620" Width="960" WindowStartupLocation="CenterScreen" FontFamily="Segoe UI">
    <Window.Background>
        <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
            <GradientStop Color="#FF1A1624" Offset="0"/>
            <GradientStop Color="#FF0E0C18" Offset="1"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <Frame x:Name="RootFrame"
               Margin="8"
               NavigationUIVisibility="Hidden"
               Background="Transparent" />
    </Grid>
</Window>
